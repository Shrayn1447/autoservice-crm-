{
   "restartable":"rs",
    "ignore":[".git", "node_modules/**/node_modules", "src/__generated__"],
    "verbose":true,
    "execMap":{
      "ts":"node --require ts_node/register"
    },
    "watch":["src"],
    "exec": "ts-node ./src/index.ts",
    "env": {
      "NODE_ENV":"development"
    },
    "ext":"js, json, ts, graphql"
}